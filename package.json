  },
  "dependencies": {
    "clean-element": "^1.0.0",
    "enzyme": "^3.3.0",
    "enzyme-to-json": "^3.3.3",
    "eslint-config-airbnb": "^15.0.2",
    "git-last-commit": "^0.3.0",
    "highcharts": "^6.0.7",
    "jest-enzyme": "^6.0.0",
    "jest-junit-reporter": "1.1.0",
    "moment": "^2.22.2",
    "numeral": "^2.0.6",
    "prop-types": "^15.6.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "react-datepicker": "^1.8.0",
